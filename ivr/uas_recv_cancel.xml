<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
<scenario name = "simple uas recv CANCEL">


<recv request="INVITE">
<action>
    <ereg regexp="[0-9]+" search_in="hdr" header="CSeq:" assign_to="inv_cseq" />
    <assignstr assign_to="local_tag" value="[call_number][pid]"/>
</action>
</recv>

<send>
<![CDATA[
    SIP/2.0 100 Trying
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<send>
<![CDATA[
    SIP/2.0 180 Ringing
    [last_Via:]
    [last_From:]
    [last_To:];tag=[$local_tag]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="CANCEL"></recv>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    CSeq: [$inv_cseq] CANCEL
    Content-Length: 0
]]>
</send>

<send>
<![CDATA[
    SIP/2.0 487 Request Cancelled
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    CSeq: [$inv_cseq] INVITE
    Content-Length: 0
]]>
</send>

<recv request="ACK"></recv>

</scenario>
