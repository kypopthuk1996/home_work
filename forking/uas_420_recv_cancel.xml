<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
<scenario name = "uas_script">
<!-- <Global variables="pointer" /> -->

<recv request="INVITE">
<action>
    <ereg regexp="Require:.*100rel.*" search_in="msg" check_it="true" assign_to="chk_req_100rel"/>
</action>
</recv>

<send>
<![CDATA[
    SIP/2.0 100 Trying
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<send>
<![CDATA[
    SIP/2.0 420 Bad Extension
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Unsupported: 100rel
    Content-Length: 0
]]>
</send>


<recv request="ACK"></recv>


<recv request="INVITE">
<action>
    <log message="INVITE  [timestamp]"/>
     <assign assign_to="chk_req_100rel" value="" />
    <ereg regexp="[0-9]+" search_in="hdr" header="CSeq:" assign_to="inv_cseq"/>
    <ereg regexp="Require:.*100rel.*" search_in="msg" check_it_inverse="true" assign_to="chk_req_100rel"/>
</action>
</recv>

<send>
<![CDATA[
    SIP/2.0 100 Trying
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<send>
<![CDATA[
    SIP/2.0 180 Ringing
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="CANCEL">
<action>
    <log message="CANCEL [timestamp]"/>
</action>
</recv>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<send>
<![CDATA[
    SIP/2.0 487 Request Cancelled
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    CSeq: [$inv_cseq] INVITE
    Content-Length: 0
]]>
</send>

<recv request="ACK"></recv>


</scenario>
