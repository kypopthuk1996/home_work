  "Tests": [
    {
      "Name": "simple_dnd_test",
      "Description": "Звоним на абонента с dnd и получаем busy",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*26#",
              "userId": 2
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "Name": "UAC_RECV_486",
              "Commands": [
                {
                  "SourceFile": "ss_dnd/uac_recv_486.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "5s"
                }
              ]
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "#26#",
              "userId": 2
            }
          ]
        }
      ]
    },
    {
      "Name": "check_dnd_white_list",
      "Description": "Звоним с абонента из белого листа, проверяем, что дозвонились. Потом делаем удаление номера из белого списка и повторяем вызов и в этот раз должны получить 486",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*26#",
              "userId": 2
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "*26*32*%%0.Number%%#",
              "userId": 2
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 2,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "10s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 0,
              "Name": "UAC_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%% -d 200",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "10s"
                }
              ]
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "#26*%%0.Number%%#",
              "userId": 2
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "Name": "UAC_RECV_486",
              "Commands": [
                {
                  "SourceFile": "ss_dnd/uac_recv_486.xml",
